{
    "manifest_version": 3,
    "name": "GitHub to CodeSandbox Redirector",
    "version": "1.0",
    "description": "Paste a GitHub repo URL to open it in CodeSandbox.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "g2c.png",
        "32": "g2c.png",
        "48": "g2c.png",
        "128": "g2c.png"
      }
    },
    "permissions": ["tabs"]
  }
  